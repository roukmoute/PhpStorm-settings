<templateSet group="Shell Script">
  <template name="xk" value="read -t $sleep&#10;xdotool key --repeat $REPEAT$ --repeat-delay $DELAY$ &quot;$END$&quot;" description="xdotools key" toReformat="false" toShortenFQNames="true">
    <variable name="REPEAT" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true" />
    <variable name="DELAY" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true" />
    <context>
      <option name="SHELL_SCRIPT" value="true" />
    </context>
  </template>
  <template name="xt" value="read -t $sleep&#10;xdotool type --delay 300 &quot;$END$&quot;;" description="xdotools type" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="SHELL_SCRIPT" value="true" />
    </context>
  </template>
  <template name="jp" value="read -t $sleep&#10;xdotool key &quot;Ctrl+semicolon&quot;&#10;read -t $sleep&#10;xdotool type --delay 300 &quot;$END$&quot;;&#10;read -t $sleep&#10;xdotool key &quot;Return&quot;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="SHELL_SCRIPT" value="true" />
    </context>
  </template>
  <template name="re" value="read -t $sleep&#10;xdotool key &quot;Ctrl+e&quot;&#10;read -t $sleep&#10;xdotool type --delay 300 &quot;$END$&quot;;&#10;read -t $sleep&#10;xdotool key &quot;Return&quot;" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="SHELL_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>